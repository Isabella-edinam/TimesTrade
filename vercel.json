{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/app", "destination": "/public/app.html" },
    { "source": "/app.html", "destination": "/public/app.html" },
    { "source": "/landing", "destination": "/public/landing.html" },
    { "source": "/landing.html", "destination": "/public/landing.html" },
    { "source": "/", "destination": "/public/landing.html" }
  ]
}