{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/app", "destination": "/public/index.html" },
    { "source": "/app.html", "destination": "/public/index.html" },
    { "source": "/", "destination": "/public/landing.html" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ]
}